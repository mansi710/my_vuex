{"ast":null,"code":"import { mapState, mapGetters, mapActions } from 'vuex';\nexport default {\n  computed: { ...mapGetters('cart', {\n      products: 'cartProducts',\n      total: 'cartTotal'\n    }),\n    ...mapState('cart', {\n      checkoutStatus: state => state.checkoutStatus\n    })\n  },\n  methods: { ...mapActions('cart', ['checkout'])\n  }\n};","map":{"version":3,"mappings":"AAeE,SAAQA,QAAR,EAAkBC,UAAlB,EAA8BC,UAA9B,QAA+C,MAA/C;AACA,eAAe;EACbC,QAAQ,EAAE,EACR,GAAGF,UAAU,CAAC,MAAD,EAAS;MACpBG,QAAQ,EAAE,cADU;MAEpBC,KAAK,EAAE;IAFa,CAAT,CADL;IAKR,GAAGL,QAAQ,CAAC,MAAD,EAAS;MAClBM,cAAc,EAAEC,KAAI,IAAKA,KAAK,CAACD;IADb,CAAT;EALH,CADG;EAUbE,OAAO,EAAE,EACP,GAAGN,UAAU,CAAC,MAAD,EAAS,CAAC,UAAD,CAAT;EADN;AAVI,CAAf","names":["mapState","mapGetters","mapActions","computed","products","total","checkoutStatus","state","methods"],"sourceRoot":"","sources":["/var/www/html/newData/mansi/vuex/src/components/ShoppingCart.vue"],"sourcesContent":["<template>\n    <div>\n      <h1>Shopping Cart</h1>\n      <ul>\n        <li v-for=\"product in products\" :key=\"\">\n          {{product.title}} - {{product.price | currency}} - {{product.quantity}}\n        </li>\n      </ul>\n      <p>Total: {{total | currency}}</p>\n      <button @click=\"checkout\">Checkout</button>\n      <p v-if=\"checkoutStatus\">{{checkoutStatus}}</p>\n    </div>\n</template>\n\n<script>\n  import {mapState, mapGetters, mapActions} from 'vuex'\n  export default {\n    computed: {\n      ...mapGetters('cart', {\n        products: 'cartProducts',\n        total: 'cartTotal'\n      }),\n      ...mapState('cart', {\n        checkoutStatus: state => state.checkoutStatus\n      })\n    },\n    methods: {\n      ...mapActions('cart', ['checkout'])\n    }\n  }\n</script>\n\n<style scoped>\n</style>\n"]},"metadata":{},"sourceType":"module"}